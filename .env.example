# Network Configuration
# Subnet ID (165 for testnet, 59 for mainnet)
NETUID=165
# Network type: 'test' or 'finney'
SUBTENSOR_NETWORK=test
#SUBTENSOR_ADDRESS=wss://test.finney.opentensor.ai:443

# Wallet Configuration
# Your wallet name
WALLET_NAME=subnet_165
# Your hotkey name
HOTKEY_NAME=default
# Path to your wallet directory
WALLET_PATH=~/.bittensor

# Port Configuration
# Port for the miner service
MINER_PORT=8082
# Port for the validator service
VALIDATOR_PORT=8081

# NATS Configuration (for validator)
# NATS server URL
NATS_URL=nats://nats:4222
# NATS channel for TEE communication
TEE_NATS_CHANNEL_NAME=miners
# Update frequency for TEE
UPDATE_TEE_CADENCE_SECONDS=5

# TEE Configuration (for miner)
# Address of the TEE worker
MINER_TEE_ADDRESS=http://tee-worker:8000
# Set to 1 for simulation mode
OE_SIMULATION=1

# System Configuration
# Enable debug logging
DEBUG=false
# API base URL
API_URL=https://test.protocol-api.masa.ai 